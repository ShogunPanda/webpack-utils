{
  "name": "@cowtech/webpack-utils",
  "version": "1.0.2",
  "description": "CowTech WebPack utility functions.",
  "homepage": "https://sw.cowtech.it/webpack-utils",
  "repository": "github:cowtech/webpack-utils",
  "keywords": [
    "webpack",
    "webpack-utils",
    "cowtech"
  ],
  "bugs": {
    "url": "https://github.com/cowtech/webpack-utils/issues"
  },
  "author": "Shogun <shogun@cowtech.it>",
  "license": "ISC",
  "private": false,
  "files": [
    "dist",
    "types",
    "CHANGELOG.md",
    "LICENSE.md",
    "README.md"
  ],
  "type": "module",
  "exports": "./dist/index.js",
  "typings": "./types/index.d.ts",
  "types": "./types/index.d.ts",
  "scripts": {
    "prebuild": "rm -rf dist types && npm run lint",
    "build": "tsc -p . && esm-pkg-add-imports-extensions dist",
    "format": "prettier -w src",
    "lint": "eslint src",
    "ci": "npm run lint",
    "prepublishOnly": "npm run ci",
    "postpublish": "git push origin && git push origin -f --tags"
  },
  "dependencies": {
    "webpack": "^5.51.1"
  },
  "devDependencies": {
    "@cowtech/eslint-config": "^8.0.1",
    "@cowtech/esm-package-utils": "^0.9.3",
    "@types/node": "^17.0.2",
    "prettier": "^2.3.2",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {
    "typescript": "^4.3.5"
  },
  "engines": {
    "node": ">=14.15.0"
  }
}
